local MainTable = {};
local Count = 0

local AddTable = {
  ["GunMods"] = "+";
  ["GodMode"] = "-";
}

for i,v in pairs(AddTable) do
    MainTable[#MainTable + 1] = {
      ResponseText = tostring(i);
      StatSign = tostring(v);
      OrderNum = Count;
    }
     Count = Count + 1
end

return MainTable;
